{
  "expo": {
    "name": "Rise",
    "slug": "riseremote",
    "scheme": "rise",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/RiseIcon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/RiseSplash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.xplatlabs.rise",
      "buildNumber": "3"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.xplatlabs.rise",
      "versionCode": 4
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "e375003a-c8cd-4eec-bec8-202333e67dc6"
      }
    },
    "owner": "finalui"
  }
}
